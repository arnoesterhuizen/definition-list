	"author": "Steven Hammond",
	"license": "MIT",
	"dependencies": {
    "@codemirror/state": "^6.2.1",
    "@codemirror/view": "^6.17.0",
    "obsidian": "^1.6.0"
  },
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}

{
	"name": "definition-list",
	"version": "0.1.1",
	"description": "Adds definition lists to the markdown parser.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
